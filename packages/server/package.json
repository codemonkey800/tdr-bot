{
  "name": "@tdr-bot/server",
  "type": "module",
  "scripts": {
    "build": "webpack --color --progress",
    "clean": "rm -rf dist",
    "dev": "pnpm clean && run-p -l dev:*",
    "dev:build": "pnpm build --watch",
    "dev:nodemon": "wait-on dist && nodemon --watch dist -e .cjs dist/main.cjs",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ext .ts src",
    "lint:prettier": "prettier --check src",
    "start": "node dist/main.cjs"
  },
  "dependencies": {
    "@discord-nestjs/common": "^5.2.10",
    "@discord-nestjs/core": "^5.3.12",
    "@mui/lab": "5.0.0-alpha.164",
    "@nestjs/common": "^10.3.2",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.3.2",
    "@nestjs/platform-express": "^10.3.2",
    "@nestjs/platform-socket.io": "^10.3.2",
    "@nestjs/schedule": "^4.0.1",
    "@nestjs/websockets": "^10.3.2",
    "@tdr-bot/shared": "workspace:*",
    "axios": "^1.6.7",
    "cheerio": "1.0.0-rc.12",
    "dedent": "^1.5.1",
    "discord.js": "^14.14.1",
    "lodash-es": "^4.17.21",
    "openai": "^4.26.1",
    "socket.io": "^4.7.4",
    "tiktoken": "^1.0.13",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "tsm": "^2.3.0"
  },
  "devDependencies": {
    "@tdr-bot/eslint-config": "workspace:*",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.16",
    "html-loader": "^5.0.0",
    "ts-loader": "^9.5.1",
    "wait-on": "^7.2.0",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  }
}
